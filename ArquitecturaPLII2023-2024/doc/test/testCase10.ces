// Flujo combinadas (si para) y bloques
*/

entero suma (entero x, entero y) {

	devuelve x+y;

}

vacio principal () {

entero a;
entero b;
entero uno;

a=0;
b=5;
uno=1;

mientras (a<5) {

	si (b<7) {
		escribe("b");
		escribeEnt(b);
	}

	escribe("a");
	escribeEnt(a);	
	b = suma (b,uno);
	a++;

}

/* bloque */
{

	entero c;
	{
		c=1;
		escribe("c");
		escribeEnt(c);
	
	}

}


devuelve;

}